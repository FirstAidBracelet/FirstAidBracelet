<!DOCTYPE html>
<html lang="en">
<head>
  <title>Database</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.0/jquery.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
</head>
<body>

<nav class="navbar navbar-default">
  <div class="container-fluid">
    <div class="navbar-header">
      <a class="navbar-brand" href="https://firstaidbracelet.herokuapp.com/">First Aid Bracelet</a>
    </div>
    <ul class="nav navbar-nav">
      <li><a href="https://firstaidbracelet.herokuapp.com/mongo"> Database</a></li>
    </ul>
  </div>
</nav>  

<div class="container">
  <h2><center> Soldiers Database <center></h2>         
  <table class="table">
    <thead> 
	<tr>
	         <% soldiers_table.forEach(function(column) { %>  
		   <td><%= column%></td>	     		   
         </tr>
		 <% }); %>
   </thead>
	  <ul>
    <% docs.forEach(function(doc) { %>           
    <tbody>
      <tr>
        <td><%= doc.personal_number%></td>
        <td><%= doc.first_name%></td>
        <td><%= doc.last_name%></td>
		<td><%= doc.unit%></td>
		<td><%= doc.division%></td>
		<td><%= doc.rank%></td>
		<td><%= doc.injury_stat%></td>
      </tr>        
	  <% }); %>
    </tbody>
  </table>
  </ul>
</div>
</body>
</html>


